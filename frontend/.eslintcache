List = useSelector(state => state.orderList)\n    const {loading, error, orders } = orderList\n    const dispatch = useDispatch()\n    const orderDelete = useSelector(state => state.orderDelete)\n    const { loading: loadingDelete, error: errorDelete, success: successDelete} = orderDelete\n    const userSignin = useSelector((state) => state.userSignin)\n    const {userInfo} = userSignin\n    \n    useEffect(() => {\n        dispatch({type: ORDER_DELETE_RESET})\n        dispatch(listOrders({ seller: sellerMode ? userInfo._id: ''}))\n        \n        \n    },[dispatch, successDelete])\n    const deleteHandler = (order) => {\n        if (window.confirm(\"Are you sure to delete?\")){\n            dispatch(deleteOrder(order._id))\n        }\n    }\n    return (\n        <div>\n            <h1>Orders</h1>\n            {loadingDelete && <LoadingBox></LoadingBox>}\n            {errorDelete && <MessageBox variant=\"danger\">{error}</MessageBox>}\n            {\n                loading? <LoadingBox></LoadingBox> \n                : error? <MessageBox variant=\"danger\">{error}</MessageBox>\n                : \n                (\n                    <table className=\"table\">\n                        <thead>\n                            <tr>\n                                <th>ID </th>\n                                <th>USER </th>\n                                <th>DATE </th>\n                                <th>TOTAL </th>\n                                <th>PAID </th>\n                                <th>DELIVERED </th>\n                                <th>ACTIONS </th>\n                            </tr>\n                        </thead>\n                        <tbody>\n                            {orders.map((order) => (\n                                <tr key={order._id}>\n                                    <td>{order._id}</td>\n                                    <td>{order.user.name}</td>\n                                    <td>{order.createdAt.substring(0,10)}</td>\n                                    <td>{order.totalPrice.toFixed(2)}</td>\n                                    <td>{order.isPaid ? order.paidAt.substring(0, 10) : 'No'}</td>\n                                    <td>{order.isDelivered ? order.deliveredAt.substring(0, 10) : 'No'} </td>\n                                    <td>\n                                        <button type=\"button\" className=\"small\" onClick={() => {props.history.push(`/order/${order._id}`)}}>\n                                            Details\n                                        </button>\n                                        <button type=\"button\" className=\"small\" onClick={() => deleteHandler(order)}>Delete</button>\n                                    </td>\n                                </tr>\n                            ))}\n                        </tbody>\n                    </table>\n                )\n            }\n        </div>\n    )\n}\n\n","/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/screens/UserListScreen.js",[],"/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/screens/UserEditScreen.js",[],"/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/components/SellerRoute.js",[],"/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/screens/SellerScreen.js",[],"/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/components/SearchBox.js",[],"/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/screens/SearchScreen.js",[],"/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/utils.js",[],"/home/arvid/Documents/amazona-Video-04-Display-Products/frontend/src/screens/MapScreen.js",[],{"ruleId":"202","replacedBy":"203"},{"ruleId":"204","replacedBy":"205"},{"ruleId":"202","replacedBy":"206"},{"ruleId":"204","replacedBy":"207"},{"ruleId":"208","severity":1,"message":"209","line":6,"column":155,"nodeType":"210","messageId":"211","endLine":6,"